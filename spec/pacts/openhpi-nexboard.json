{
  "consumer": {
    "name": "OpenHPI"
  },
  "provider": {
    "name": "nexboard"
  },
  "interactions": [
    {
      "description": "a request to get all projects",
      "providerState": "user has valid credentials and some projects exist",
      "request": {
        "method": "get",
        "path": "/projects",
        "query": "token=RandomApiKey&userId=1337"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": [
          {
            "project_id": "1"
          },
          {
            "project_id": "2"
          }
        ]
      }
    },
    {
      "description": "a request to create a project",
      "providerState": "user has valid credentials",
      "request": {
        "method": "post",
        "path": "/projects",
        "query": "token=RandomApiKey",
        "body": {
          "title": "A new project",
          "description": "This is a new project",
          "user_id": "1337"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "project_id": "1",
          "title": "A new project"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}